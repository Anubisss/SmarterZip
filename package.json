{
  "name": "smarterzip",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "scheduler:dev": "nodemon --watch src/scheduler --watch src/lib --exec \"tsx --tsconfig tsconfig.scheduler.json --env-file=.env\" src/scheduler/index.ts",
    "scheduler:prod": "node dist/scheduler/scheduler/index.js",
    "scheduler:build": "tsc -p tsconfig.scheduler.json",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit && tsc -p tsconfig.scheduler.json --noEmit",
    "check-all": "rm -rf .next && npm run lint && npm run type-check && npm run prettier && npm run build && npm run scheduler:build && npm audit && npm run test",
    "prepare": "husky",
    "prettier": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.66.0",
    "@tanstack/react-query-devtools": "^5.75.5",
    "axios": "^1.7.2",
    "axios-cookiejar-support": "^5.0.2",
    "better-sqlite3": "^11.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "next": "^14.2.23",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.3.0",
    "react-moment": "^1.1.3",
    "tough-cookie": "^4.1.4"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.74.7",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/better-sqlite3": "^7.6.11",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/tough-cookie": "^4.0.5",
    "eslint": "^8",
    "eslint-config-next": "^14.2.13",
    "eslint-plugin-jest": "^28.11.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.1.7",
    "postcss": "^8",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5",
    "typescript-eslint": "^8.7.0"
  }
}
